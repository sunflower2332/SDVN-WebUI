def cloudflare_thread(port):
  import socket
  while True:
      time.sleep(0.5)
      sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
      result = sock.connect_ex(('127.0.0.1', port))
      if result == 0:
        break
      sock.close()

  p = subprocess.Popen(["cloudflared", "tunnel", "--url", "http://127.0.0.1:{}".format(port)], stdout=subprocess.PIPE, stderr=subprocess.PIPE)
  for line in p.stderr:
    l = line.decode()
    if "trycloudflare.com " in l:
      print(f"\033[92m{'üîó ComfyUI Cloudflare URL:'}\033[0m", l[l.find("http"):], flush=True)
      html="<div><code style='color:yellow'>B·∫°n ƒëang ch·∫°y trong ch·∫ø ƒë·ªô FastMode ƒë·ªÉ tƒÉng t·ªëc ƒë·ªô kh·ªüi ƒë·ªông, ch·∫ø ƒë·ªô n√†y s·∫Ω t·∫£i √≠t node h∆°n, kh√¥ng t·∫£i s·∫µn c√°c lo·∫°i controlnet/ipadapter... B·ªè ch·ªçn FastMode ho·∫∑c ch·ªçn Preset kh√°c v√† ch·∫°y l·∫°i ph·∫ßn 1 ƒë·ªÉ t·∫£i ƒë·∫ßy ƒë·ªß theo nhu c·∫ßu</code></div>"
